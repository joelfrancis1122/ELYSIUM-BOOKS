    <script>
        // Function to increase quantity
        function increaseQuantity(button) {
            var quantitySpan = button.previousElementSibling;
            var currentQuantity = parseInt(quantitySpan.textContent);
            quantitySpan.textContent = currentQuantity + 1;
        }

        // Function to decrease quantity
        function decreaseQuantity(button) {
            var quantitySpan = button.nextElementSibling;
            var currentQuantity = parseInt(quantitySpan.textContent);
            if (currentQuantity > 1) {
                quantitySpan.textContent = currentQuantity - 1;
            }
        }
    </script>
  <script>
    function increase(productId) {
        const quantityElement = document.getElementById(`quantity_${productId}`);
        const currentQuantity = parseInt(quantityElement.textContent);
        const stockElement = document.getElementById(`stock_${productId}`);
        const stock = parseInt(stockElement.textContent);

        if (currentQuantity >= stock) {
            toastr.error("Sorry, the maximum stock limit exceeded.");
            return; // Exit the function early if stock limit exceeded
        }

        fetch('/updateCart', {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json'
            },
            body: JSON.stringify({
                productId: productId,
                quantity: 1 // Increase by 1
            })
        })
        .then(response => {
            if (response.ok) {
                // Increment the quantity displayed on the page
                quantityElement.textContent = currentQuantity + 1;

                // Update the subtotal with two decimal places
                const salePrice = document.getElementById(`saleprice_${productId}`).textContent;
                const subtotalElement = document.getElementById(`subtotal_${productId}`);
                const saleP = parseFloat(salePrice.replace('$', ''));
                const subtotal = (saleP * (currentQuantity + 1)).toFixed(2); // Format to two decimal places
                subtotalElement.textContent = `$${subtotal}`;

                updateTotalPrice();
            } else {
                console.error('Failed to update cart');
            }
        })
        .catch(error => {
            console.error('Error:', error);
        });
    }
</script>


<script>
    function decrease(pId) {
        const quantityElement = document.getElementById('quantity_' + pId);
        const currentQuantity = parseInt(quantityElement.textContent);
        if (currentQuantity > 1) {
            fetch('/updateCart', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify({
                    productId: pId,
                    quantity: -1 // Decrease by 1
                })
            })
            .then(response => {
                if (response.ok) {
                    // Decrement the quantity displayed on the page
                    quantityElement.textContent = currentQuantity - 1;
                    // Update the subtotal
                    const subtotalElement = document.getElementById('subtotal_' + pId);
                    const salePrice = document.getElementById('saleprice_' + pId);
                    let saleP = parseFloat(salePrice.textContent.replace('$', ''));
                    // Calculate the new subtotal with two decimal places
                    const newSubtotal = (saleP * (currentQuantity - 1)).toFixed(2);
                    subtotalElement.textContent = `$${newSubtotal}`;
                    updateTotalPrice();
                } else {
                    console.error('Failed to update cart');
                }
            })
            .catch(error => {
                console.error('Error:', error);
            });
        } else {
            // Quantity is 1, do nothing or display a message
            console.log('Cannot decrease further. Quantity is already 1.');
        }
    }

