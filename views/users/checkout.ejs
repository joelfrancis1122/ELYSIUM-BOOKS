    const orderData = {
        paymentMethod: selectedRadioButton,
        amount: totalAmount,
        addresss:selectedAddress
    };

    // Send a POST request to place the order
    fetch('/placeorder', {
        method: 'POST',
        headers: {
            'Content-Type': 'application/json',
        },
        body: JSON.stringify(orderData),
    })
    .then(response => response.json())
    .then(data => {
        Swal.fire({
            position: "center",
            icon: "success",
            title: "Order Placed",
            showConfirmButton: false,
            timer: 2000
        });
        // Chain the removeItem function here
        removeItem(); // This will execute removeItem after placeOrder is successful
